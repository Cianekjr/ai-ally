{
  "extends": "tsconfig/server.json",
  "compilerOptions": {
    "outDir": "./dist",
    "baseUrl": "./",
    "lib": [
      "ES2021"
    ],
    "target": "ES2021",
    "isolatedModules": true,
    "moduleResolution": "node",
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
  ],
  "exclude": [
    "dist",
    "node_modules"
  ]
}