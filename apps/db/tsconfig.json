{
  // "extends": "tsconfig/server.json",
  "compilerOptions": {
    "module": "commonjs",
    "removeComments": true,
    "allowSyntheticDefaultImports": false,
    "outDir": "./dist",
    "baseUrl": "./",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "lib": [
      "ES2021"
    ],
    "target": "ES2021",
    "isolatedModules": true,
    "moduleResolution": "node",
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
  ],
  "exclude": [
    "dist",
    "node_modules"
  ]
}