{
  "name": "ai-ally",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "engines": {
    "node": ">=16.8.0"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "prettier": "2.8.4",
    "prettier-config-custom": "workspace:*"
  },
  "packageManager": "pnpm@7.27.1",
  "dependencies": {
    "turbo": "^1.6.3"
  }
}