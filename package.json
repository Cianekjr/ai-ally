{
  "name": "guide-me-now",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "build:client": "turbo build --filter=client",
    "build:server": "turbo build --filter=server --force",
    "heroku-postbuild": "if [ $CLIENT_ENV ]; then pnpm build:client; elif [ $SERVER_ENV ]; then pnpm build:server; else echo no environment detected, please set CLIENT_ENV or SERVER_ENV; fi"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "prettier": "2.8.4",
    "prettier-config-custom": "workspace:*",
    "turbo": "1.8.2"
  },
  "packageManager": "pnpm@7.27.1"
}